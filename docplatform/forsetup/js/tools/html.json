{
    "css": [
        "html {",
        "  box-sizing: border-box;",
        "}",
        "*, *:before, *:after {",
        "  box-sizing: inherit;",
        "}",
        ".stl-inline-item {",
        "  display: inline-block;",
        "  vertical-align: text-bottom;",
        "}",
        "*[data-stl-class=\"stl:page\"] {",
        "  position: relative;",
        "}",
        "*[data-stl-class=\"stl:text\"] {",
        "  overflow: hidden;",
        "}",
        "*[data-stl-class=\"stl:table\"] {",
        "  border-collapse: collapse;",
        "}"
    ]
}
