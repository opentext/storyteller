{
    "css": [
        "html {",
        "  box-sizing: border-box;",
        "}",
        "*, *:before, *:after {",
        "  box-sizing: inherit;",
        "}",
        ".stl-inline-item {",
        "  display: inline-block;",
        "  vertical-align: text-bottom;",
        "}",
        ".stl-image {",
        "  width: 100%;",
        "  height: 100%;",
        "}",
        "*[data-stl-class=\"stl:page\"] {",
        "  position: relative;",
        "}",
        "*[data-stl-class=\"stl:text\"] {",
        "  overflow: hidden;",
        "}"
    ]
}
