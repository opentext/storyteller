<stl:stl xmlns:stl="http://developer.opentext.com/schemas/storyteller/layout" version="0.1">
  <stl:style>
    .span1 {
      font-family: Lato;
      font-size: 10pt
    }
    .p1 {
      text-align: center
    }
    .p2 {
      text-align: right
    }
    .p3 {
      text-align: justify
    }
    .p4 {
      line-height: 0.15in;
      margin-bottom: 0.2in;
      margin-left: 0.3in;
      margin-right: 0.4in;
      margin-top: 0.1in;
      text-align: justify
    }
    .p5 {
      margin-left: 0.25in
    }
    .p6 {
      margin-left: 0.5in
    }
    .p7 {
      margin-left: 0.75in
    }
    .p8 {
      -stl-list-level: 0;
      -stl-list-mask: &#x2022;\9;
      -stl-tabs: 0.25in;
      margin-left: 0in
    }
    .p9 {
      -stl-list-level: 1;
      -stl-list-mask: &#x25E6;\9;
      -stl-tabs: 0.5in;
      margin-left: 0.25in
    }
    .p10 {
      -stl-list-level: 2;
      -stl-list-mask: &#x25AA;\9;
      -stl-tabs: 0.75in;
      margin-left: 0.5in
    }
    .p11 {
      -stl-list-level: 3;
      -stl-list-mask: &#x2022;\9;
      -stl-tabs: 1in;
      margin-left: 0.75in
    }
    .p12 {
      -stl-list-counter: default_counter;
      -stl-list-level: 0;
      -stl-list-mask: %0!1.\9;
      -stl-tabs: 0.25in;
      margin-left: 0in
    }
    .p13 {
      -stl-list-counter: default_counter;
      -stl-list-level: 1;
      -stl-list-mask: %1!1.\9;
      -stl-tabs: 0.5in;
      margin-left: 0.25in
    }
    .p14 {
      -stl-list-counter: default_counter;
      -stl-list-level: 2;
      -stl-list-mask: %2!r.\9;
      -stl-tabs: 0.75in;
      margin-left: 0.5in
    }
    .p15 {
      -stl-list-counter: default_counter;
      -stl-list-level: 3;
      -stl-list-mask: %3!1)\9;
      -stl-tabs: 1in;
      margin-left: 0.75in
    }
  </stl:style>
  <stl:document>
    <stl:story name="Main" w="4.2in">
      <stl:p><stl:span class="span1">Left justification</stl:span></stl:p>
      <stl:p class="p1"><stl:span class="span1">Center justification</stl:span></stl:p>
      <stl:p class="p2"><stl:span class="span1">Right justification</stl:span></stl:p>
      <stl:p class="p3"><stl:span class="span1">Full justification</stl:span></stl:p>
      <stl:p class="p4"><stl:span class="span1">Paragraph and line spacing</stl:span></stl:p>
      <stl:p><stl:span class="span1">No Indent</stl:span></stl:p>
      <stl:p class="p5"><stl:span class="span1">Indent</stl:span></stl:p>
      <stl:p class="p6"><stl:span class="span1">Indent more</stl:span></stl:p>
      <stl:p class="p7"><stl:span class="span1">Indent even more</stl:span></stl:p>
      <stl:p class="p8"><stl:span class="span1">Bullets level 1</stl:span></stl:p>
      <stl:p class="p9"><stl:span class="span1">Bullets level 2</stl:span></stl:p>
      <stl:p class="p10"><stl:span class="span1">Bullets level 3</stl:span></stl:p>
      <stl:p class="p11"><stl:span class="span1">Bullets level 4</stl:span></stl:p>
      <stl:p class="p12"><stl:span class="span1">Numbering level 1</stl:span></stl:p>
      <stl:p class="p13"><stl:span class="span1">Numbering Level 2</stl:span></stl:p>
      <stl:p class="p14"><stl:span class="span1">Numbering Level 3</stl:span></stl:p>
      <stl:p class="p15"><stl:span class="span1">Numbering Level 4</stl:span></stl:p>
    </stl:story>
  </stl:document>
</stl:stl>