<stl:stl xmlns:stl="http://developer.opentext.com/schemas/storyteller/layout" version="0.1">
  <stl:style>
    .text1 {
      -stl-shape-resize: free 0pt max 0pt max;
      border: 1px solid #000000
    }
    .span1 {
      font-family: Lato;
      font-size: 10pt
    }
    .table1 {
      border: 0.001in solid #000000;
      display: table
    }
    .row1 {
      -stl-shape-resize: free 0pt max 0pt max
    }
    .p1 {
      text-align: center
    }
  </stl:style>
  <stl:document>
    <stl:page w="3.6in" h="1in">
      <stl:text x="1in" y="0.5in" w="1in" h="0.198in" class="text1">
        <stl:story>
          <stl:p><stl:span class="span1">Hello</stl:span><stl:table class="table1">
              <stl:story>
                <stl:row h="0.25in" class="row1">
                  <stl:cell w="0.477in">
                    <stl:p class="p1"><stl:span class="span1">Open</stl:span></stl:p>
                  </stl:cell>
                  <stl:cell w="0.477in">
                    <stl:p class="p1"><stl:span class="span1">text</stl:span></stl:p>
                  </stl:cell>
                </stl:row>
              </stl:story>
            </stl:table></stl:p>
        </stl:story>
      </stl:text>
    </stl:page>
  </stl:document>
</stl:stl>