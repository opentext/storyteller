<stl:stl xmlns:stl="http://developer.opentext.com/schemas/storyteller/layout" version="0.1">
  <stl:document>
    <stl:story name="Main" w="8.5in">
      <stl:p>
        <stl:span style="font-family: Lato; font-size: 10pt">Hello</stl:span>
      </stl:p>
      <stl:p>
        <stl:table style="border: 0.001in solid #000000">
          <stl:story>
            <stl:row h="0.25in" style="-stl-shape-resize: free; -stl-shape-growth: 0pt max; -stl-shape-shrink: 0pt -max">
              <stl:cell w="1in">
                <stl:p style="text-align: center">
                  <stl:span style="font-family: Lato; font-size: 10pt">Open</stl:span>
                </stl:p>
              </stl:cell>
              <stl:cell w="1in">
                <stl:p style="text-align: center">
                  <stl:span style="font-family: Lato; font-size: 10pt">Text</stl:span>
                </stl:p>
              </stl:cell>
            </stl:row>
          </stl:story>
        </stl:table>
      </stl:p>
      <stl:p>
        <stl:span style="font-family: Lato; font-size: 10pt">!</stl:span>
      </stl:p>
    </stl:story>
  </stl:document>
</stl:stl>